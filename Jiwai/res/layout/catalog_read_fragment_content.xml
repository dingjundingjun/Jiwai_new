<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="210dp"
        >
        <ImageView 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/find_background"
            android:background="@drawable/find_background"
            />
    </RelativeLayout>
    <com.sounuo.jiwai.views.TitleScroolView
        android:id="@+id/titlescrool"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="@dimen/seeworld_title_height"
        />
    <com.sounuo.jiwai.views.FragmentViewPaper
        android:id="@+id/viewpager"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:persistentDrawingCache="animation"/>
</LinearLayout>